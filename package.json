  "devDependencies": {
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-cli": "^26.1.0",
    "lint-staged": "^10.2.11",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
